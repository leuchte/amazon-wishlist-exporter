{
  "name": "schmiddim/amazon-wish-lister",
  "description": "Simple script that parses a public amazon wishlist and export the items to a CSV file.",
  "keywords": [
    "amazon",
    "wishlist",
    "export",
    "csv"
  ],
  "bin": [
    "awx"
  ],
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-0": {
      "AmazonWishlistExporter": "src/",
      "AmazonWishlistExporter\\Test": "test/"
    }
  },
  "require": {
    "guzzlehttp/guzzle": "*",
    "symfony/dom-crawler": "*",
    "symfony/css-selector": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*"
  }
}
